{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg))",
            "headers": {
                "Cache-Control": "public, max-age=86400"
            }
        }
    ]
}